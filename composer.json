{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.2.*",
    "doctrine/dbal": "^2.5",
    "predis/predis": "1.1.*@dev",
    "torann/registry": "0.2.*@dev",
    "tshafer/laravel-dev": "1.*",
    "laravelcollective/html": "5.2.*",
    "tshafer/laravel-flash": "1.*",
    "tshafer/laravel-support": "dev-master",
    "rap2hpoutre/laravel-log-viewer": "^0.4.3",
    "spatie/laravel-medialibrary": "^3.0",
    "lookitsatravis/listify": "^1.2",
    "gurzhii/laravel-sluggable-trait": "^1.0",
    "baum/baum": "^1.1",
    "iatstuti/laravel-cascade-soft-deletes": "1.0.*",
    "radic/blade-extensions": "^6.2",
    "gloudemans/shoppingcart": "^2.1",
    "league/fractal": "^0.13.0",
    "nicolaslopezj/searchable": "1.*"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "symfony/css-selector": "2.8.*|3.0.*",
    "symfony/dom-crawler": "2.8.*|3.0.*",
    "laracasts/generators": "^1.1"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "app/Support/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
