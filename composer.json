{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "5.2.*",
    "doctrine/dbal": "^2.5",
    "predis/predis": "1.1.*@dev",
    "torann/registry": "0.2.*@dev",
    "tshafer/laravel-dev": "1.*",
    "laravelcollective/html": "5.2.*",
    "tshafer/laravel-flash": "1.*",
    "tshafer/laravel-support": "dev-master",
    "guzzlehttp/guzzle": "~5.0",
    "radic/blade-extensions": "~6.0",
    "spatie/laravel-medialibrary": "^3.3",
    "spatie/laravel-backup": "^2.7",
    "rap2hpoutre/laravel-log-viewer": "^0.4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "app/Support/helpers.php"
    ],
    "psr-4": {
      "Wash\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
